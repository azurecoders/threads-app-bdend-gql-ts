generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String  @id @default(uuid())
  firstName String  @map("first_name")
  lastName  String? @map("last_name")
  email     String  @unique
  password  String
  salt      String

  @@map("users")
}
